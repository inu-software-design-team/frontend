{
  "name": "software-design",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate-icons": "tsx ./src/generate-icons.ts",
    "dev": "npm run generate-icons && next dev --turbopack",
    "build": "npm run generate-icons && next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "storybook": "npm run generate-icons && storybook dev -p 6006",
    "build-storybook": "npm run generate-icons && storybook build --stats-json"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "es-toolkit": "^1.43.0",
    "lucide-react": "^0.553.0",
    "next": "^15.5.9",
    "pretendard": "^1.3.9",
    "react": "^19.1.1",
    "react-calendar": "^6.0.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.7",
    "@eslint/eslintrc": "^3.3.3",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/blocks": "^8.6.12",
    "@storybook/nextjs": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^0.12.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "storybook": "^8.6.15",
    "tailwindcss": "^4.1.4",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "overrides": {
    "yaml": "^2.7.1"
  }
}
